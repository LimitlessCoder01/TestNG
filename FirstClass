package org.hij;

import org.testng.annotations.AfterClass;
import org.testng.annotations.AfterMethod;
import org.testng.annotations.AfterSuite;
import org.testng.annotations.AfterTest;
import org.testng.annotations.BeforeClass;
import org.testng.annotations.BeforeMethod;
import org.testng.annotations.BeforeSuite;
import org.testng.annotations.BeforeTest;
import org.testng.annotations.Test;

public class NewTest {
  
	@BeforeSuite
	public void testOne() {
		System.out.println("Class Test 1");
	}
	
	@AfterSuite
	public void testTwo() {
		System.out.println("Class Test 2");
	}
	
	@BeforeClass
	public void testThree() {
		System.out.println("Class Test 3");
	}
	
	@AfterClass
	public void testFour() {
		System.out.println("Class Test 4");
	}
	
	@BeforeMethod
	public void testFive(){
		System.out.println("Class Test 5");
	}
	
	@AfterMethod
	public void testSix(){
		System.out.println("Class Test 6");
	}
	
	@BeforeTest
	public void testSeven() {
		System.out.println("Class Test 7");
	}
	
	@AfterTest
	public void testEight() {
		System.out.println("Class Test 8");
	}
	
	@Test
	public void testNine() {
		System.out.println("Class Test 9");
	}

}
